<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns:context="http://www.springframework.org/schema/context" 
 xmlns:p="http://www.springframework.org/schema/p" 
 xmlns:c="http://www.springframework.org/schema/c" 
 xsi:schemaLocation="http://www.springframework.org/schema/beans
                    http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
                    http://www.springframework.org/schema/context 
                    http://www.springframework.org/schema/context/spring-context-4.0.xsd">
                    
                    <bean class="com.spring.BeanScope.Address" name="address" p:city="Gorakhpur" p:state="Bihar" scope="prototype"/>
                    <bean class="com.spring.BeanScope.Student" name="student" p:id="1" p:name="Prince Kumar" p:address-ref="address" scope="prototype"/>
                    
                    
                    </beans>